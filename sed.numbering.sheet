  \r\n # number each line of a file (number on left, right-aligned)\r\n sed = filename | sed 'N; s/^/     /; s/ *\\(.\\{6,\\}\\)\\n/\\1  /'\r\n\
  \r\n # number each line of file, but only print numbers if line is not blank\r\n sed '/./=' filename | sed '/./N; s/\\n/ /'\r\n\
  \r\n # count lines (emulates \"wc -l\")\r\n sed -n '$='"
